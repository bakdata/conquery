{
  "name": "conquery",
  "version": "1.2.0",
  "description": "conquery",
  "main": "dist/js/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0 <9.0.0",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "start": "node app/server",
    "start-production": "NODE_ENV=production PORT=8000 node app/server > webapp.log 2> webapp-err.log &",
    "lint": "eslint --ext=js lib/js test app/src/js",
    "build": "rimraf dist && gulp",
    "build-app": "cd app && rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "heroku-postbuild": "yarn run build-app",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "rimraf coverage && NODE_ENV=test nyc _mocha test",
    "test-watch": "NODE_ENV=test mocha test --watch",
    "debug-test": "NODE_ENV=test node-debug --debug-brk _mocha test",
    "prepush": "npm run lint"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.5.3",
    "empty": "^0.10.1",
